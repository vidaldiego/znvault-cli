{
  "name": "@zincapp/znvault-cli",
  "version": "2.6.2",
  "description": "Official CLI for ZN-Vault administration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "znvault": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "pretest": "npm run build",
    "test": "../scripts/sdk-test-run.sh npm run test:all",
    "test:all": "vitest run --config vitest.integration.config.ts",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "ZNVAULT_INTEGRATION=true vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "znvault",
    "vault",
    "secrets",
    "cli",
    "admin"
  ],
  "author": "ZincApp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vidaldiego/znvault-cli"
  },
  "dependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^3.0.3",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "ink": "^6.6.0",
    "ink-text-input": "^6.0.0",
    "ink-ui": "^0.4.0",
    "inquirer": "^13.1.0",
    "openpgp": "^6.3.0",
    "ora": "^9.0.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
